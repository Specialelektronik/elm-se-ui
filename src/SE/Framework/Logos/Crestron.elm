{-| Crestron logos
-}

import Html.Styled exposing (Html)
import Svg.Styled as Svg
import Svg.Styled.Attributes as Attributes exposing (d, fill, fillRule, viewBox)


type Color
    = Blue
    | Black
    | White


blue : Html msg
blue =
    logo Blue


black : Html msg
black =
    logo Black


white : Html msg
white =
    logo White


blueStack : Html msg
blueStack =
    stack Blue


blackStack : Html msg
blackStack =
    stack Black


whiteStack : Html msg
whiteStack =
    stack White


blueSwirl : Html msg
blueSwirl =
    swirl Blue


blackSwirl : Html msg
blackSwirl =
    swirl Black


whiteSwirl : Html msg
whiteSwirl =
    swirl White


toHex : Color -> String
toHex c =
    case c of
        Blue ->
            "#124169"

        Black ->
            "#000"

        White ->
            "#fff"


